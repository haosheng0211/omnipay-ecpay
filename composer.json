{
    "name": "omnipay-taiwan/ecpay",
    "description": ":package_description",
    "keywords": [
        "omnipay",
        "ecpay"
    ],
    "homepage": "https://github.com/omnipay-taiwan/omnipay-ecpay",
    "license": "MIT",
    "authors": [
        {
            "name": "recca0120",
            "email": "recca0120@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "ecpay/php",
                "version": "1.1.2",
                "dist": {
                    "url": "https://github.com/ECPay/ECPayAIO_PHP/archive/refs/heads/master.zip",
                    "type": "zip"
                },
                "autoload": {
                    "classmap": [
                        "AioSDK/sdk",
                        "ApplePaySDK/sdk",
                        "EInvoiceSDK/sdk",
                        "PayLogisticSDK/sdk"
                    ]
                }
            }
        }
    ],
    "require": {
        "omnipay/common": "^3.0",
        "ecpay/php": "^1.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "omnipay/tests": "^3.0",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\ECPay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\ECPay\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
